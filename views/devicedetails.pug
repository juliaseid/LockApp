extends layout

block content
  h2= title

  h3 #{deviceName}
  p Your device is #{lockStatus}
  a(class='button' href=`/devicedetails/${deviceId}/set_locked`) Lock 
  button(type='button' onclick='setUnlocked()') Unlock
  p Your device is #{jammedStatus}
  p Your battery is #{batteryLevel}% charged 
  ul PIN Codes 
  each name in pinCodeNames 
    li= name